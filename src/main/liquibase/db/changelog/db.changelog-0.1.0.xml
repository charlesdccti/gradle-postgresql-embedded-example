<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd"
        logicalFilePath="db.changelog-0.1.0.xml">

    <changeSet id="1" author="nihilist">
        <createTable tableName="bookmarks">
            <column name="id" autoIncrement="true" type="SERIAL">
                <constraints primaryKey="true"/>
            </column>
            <column name="uri" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="VARCHAR"/>
        </createTable>
    </changeSet>

    <changeSet id="tag-0.1.0" author="nihilist">
        <tagDatabase tag="0.1.0"/>
    </changeSet>
</databaseChangeLog>
